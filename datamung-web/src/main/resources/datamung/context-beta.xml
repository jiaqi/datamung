<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:jaxrs="http://cxf.apache.org/jaxrs" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
       http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://cxf.apache.org/jaxrs
       http://cxf.apache.org/schemas/jaxrs.xsd">
    <bean id="awsCredentials"
        class="com.amazonaws.auth.InstanceProfileCredentialsProvider" />
    <jaxrs:client id="dataMungService"
        serviceClass="org.cyclopsgroup.datamung.api.DataMungService"
        address="http://cg-datamung-srv-beta.elasticbeanstalk.com/v1/" />
</beans>